  "engines": {
    "node": ">=18.0",
    "npm": ">=8.6"
  },
  "overrides": {
    "uuid": "$uuid"
  },
  "scripts": {
    "dev": "next dev",
    "test": "npm run build && playwright test",
    "lint": "eslint . --config .eslintrc.json --ext .js,.jsx,.ts,.tsx",
    "build": "next build && next-sitemap",
    "start": "pm2 start --name domego next -- start -p 3000",
    "stop": "pm2 delete domego",
    "restart": "pm2 restart domego --update-env",
    "reload": "pm2 reload domego",
    "tunnel": "npx localtunnel --port 3000"
  },
  "browserslist": [
    "defaults"
  ]
